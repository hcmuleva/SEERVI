    "jest": "^24.9.0",
    "jest-cli": "^25.1.0",
    "jest-runner-groups": "^1.0.0",
    "jest-stare": "^1.26.0",
    "jsonwebtoken": "^8.5.1",
    "lowdb": "^1.0.0",
